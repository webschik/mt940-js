{
  "name": "@jewell-lgtm/mt940-js",
  "version": "2.0.0-alpha.4",
  "description": "A Node.js library for working with MT940 format",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "coveralls": "^3.0.9",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "tslint": "^6.0.0",
    "tslint-config-webschik": "^1.0.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "ci": "RUN_PERFORMANCE_TESTS=1 jest",
    "prepare": "npm run build",
    "test": "jest",
    "test:performance": "RUN_PERFORMANCE_TESTS=1 jest",
    "snapshot:update": "RUN_PERFORMANCE_TESTS=1 WRITE_SNAPSHOT=1 jest",
    "lint": "tslint \"src/**/*.ts\" \"__tests__/unit/**/*.ts\"",
    "lint:fix": "tslint --fix \"src/**/*.ts\" \"__tests__/unit/**/*.ts\"",
    "format": "prettier --check '{src,__tests__/unit}/**/*.{js,ts}' && prettier --write '*.{js,ts}'",
    "format:fix": "prettier --write '{src,__tests__/unit}/**/*.{js,ts}' && prettier --write '*.{js,ts}'",
    "build": "rm -rf lib && tsc"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jewell-lgtm/mt940-js.git"
  },
  "keywords": [
    "nodeJS",
    "mt940"
  ],
  "author": "webschik",
  "license": "MIT",
  "dependencies": {}
}
